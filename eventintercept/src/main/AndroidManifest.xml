<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventintercept">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.FirstActivity" />
        <activity android:name=".activities.SecondActivity" />
        <activity android:name=".activities.ViewPagerInScrlloer" />
        <activity android:name=".activities.FourActivity" />
        <activity android:name=".activities.ViewPagerActivity" />
        <activity android:name=".activities.SixActivity"></activity>
    </application>

</manifest>